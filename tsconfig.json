{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "noEmit": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "types": [
      "cypress",
      "node",
      "TestConfigOverrides",
      "cypress-file-upload",
      "browserPermissions"
    ],
    "allowJs": true
  },
  "include": [
    "**/*.ts",
    "cypress/support/index.js",
    "cypress.config.js",
    "../node_modules/@shelex/cypress-allure-plugin/reporter"
  ]
}
